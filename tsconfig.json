{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "jsx": "react-native",
        "lib": ["dom", "esnext"],
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "noImplicitAny": true,
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "*": ["src/*"]
        },
        "allowJs": true
    },
    "extends": "expo/tsconfig.base"
}
